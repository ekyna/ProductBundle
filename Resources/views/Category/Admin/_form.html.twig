<ul class="nav nav-tabs">
    <li class="active"><a href="#product-cate-general" data-toggle="tab">{{ 'ekyna_core.field.general'|trans }}</a></li>
    <li><a href="#product-cate-seo" data-toggle="tab">{{ 'ekyna_core.field.seo'|trans }}</a></li>
</ul>
<div class="panel-body form-body">
    {{ bootstrap_set_style('horizontal') }}
    <div class="tab-content">
	    <div class="tab-pane active" id="product-cate-general">
	        {{ form_row(form.name) }}
	        {{ form_row(form.parent) }}
	        {% if form.html is defined %}
	            {{ form_row(form.html) }}
	        {% endif %}
	        {{ form_row(form.image) }}
	    </div>
	    <div class="tab-pane" id="product-cate-seo">
	        {{ form_row(form.seo.title) }}
	        {{ form_row(form.seo.description) }}
	        <div class="row">
	            <div class="col-md-6">{{ form_row(form.seo.changefreq, {'label_col': 4, 'widget_col': 8}) }}</div>
	            <div class="col-md-6">{{ form_row(form.seo.priority, {'label_col': 4, 'widget_col': 8}) }}</div>
	        </div>
	    </div>
    </div>
    {{ form_rest(form) }}
</div>